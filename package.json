{
  "name": "turborepo-shadcn-ui",
  "version": "1.2.1",
  "private": true,
  "license": "MIT",
  "scripts": {
    "build": "turbo build",
    "dev": "turbo dev",
    "lint": "turbo lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "ui:add": "pnpm --filter @repo/ui ui:add",
    "dev:docker": "pnpm run prisma:docker && pnpm run dev",
    "prisma:docker": "cd packages/db && DATABASE_URL=\"postgresql://postgres:mysecretpassword@db:5432/find-it-here?schema=public\" npx prisma generate && npx prisma migrate deploy",
    "start:web": "cd apps/web && pnpm run start",
    "start": "cd apps/docs && pnpm start",
    "build:docs": "cd apps/docs && pnpm run build",
    "prisma:generate": "cd packages/db && npx prisma generate",
    "db:migrate": "cd packages/db && npx prisma migrate deploy",
    "vercel-build": "pnpm prisma:generate && pnpm build"
  },
  "devDependencies": {
    "@repo/eslint-config": "workspace:*",
    "@repo/typescript-config": "workspace:*",
    "@types/lodash": "^4.17.13",
    "autoprefixer": "^10.4.19",
    "axios": "^1.7.7",
    "framer-motion": "^11.3.24",
    "lodash": "^4.17.21",
    "prettier": "^3.3.2",
    "puppeteer": "^23.3.0",
    "turbo": "latest"
  },
  "packageManager": "pnpm@8.9.0",
  "engines": {
    "node": ">=18"
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "react",
        "react-dom"
      ]
    }
  },
  "dependencies": {
    "@date-io/date-fns": "^3.0.0",
    "@emotion/react": "^11.13.3",
    "@emotion/styled": "^11.13.0",
    "@google/generative-ai": "^0.2.1",
    "@headlessui/react": "^2.1.8",
    "@icons-pack/react-simple-icons": "^9.5.0",
    "@mui/material": "^6.1.1",
    "@mui/x-date-pickers": "^7.18.0",
    "@prisma/client": "^5.15.0",
    "@tabler/icons-react": "^3.12.0",
    "@types/dompurify": "^3.0.5",
    "@types/html-to-text": "^9.0.4",
    "@types/react-alice-carousel": "^1.15.3",
    "@types/wkhtmltopdf": "^0.3.9",
    "ai": "^3.3.0",
    "date-fns": "^4.1.0",
    "dayjs": "^1.11.13",
    "dompurify": "^3.1.6",
    "lucide-react": "^0.395.0",
    "next-themes": "^0.3.0",
    "react-alice-carousel": "^2.9.1",
    "react-dropzone": "^14.2.10",
    "react-icons": "^5.3.0",
    "react-quill": "^2.0.0",
    "react-responsive-modal": "^6.4.2",
    "react-to-pdf": "^1.0.1",
    "react-tooltip": "^5.28.0",
    "sass": "^1.77.8",
    "typewriter-effect": "^2.21.0"
  }
}